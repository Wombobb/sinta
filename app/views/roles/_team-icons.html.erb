<% @users.each do |user| %>
  <%# <img src="https://mdbcdn.b-cdn.net/img/new/avatars/8.webp" class="rounded-circle mb-3" style="width: 150px;"
          alt="Avatar" /> %>
  <%= image_tag current_user.photo, alt: "text", class: "avatar-bordered" %>
  <h5 class="mb-2"><strong><%= user.first_name.capitalize %></strong></h5>
  <p class="text-muted"><%= user.position.capitalize %><span class="badge bg-primary">PRO</span></p>
<% end %>
